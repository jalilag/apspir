set terminal pngcairo size 1000,100 enhanced font 'Verdana,10'
set output 'temp.png'
set key off
set style line 1 lc rgb '#5e9c36' pt 6 ps 1 lt 1 lw 2
set style line 2 lc rgb '#8b1a0e' pt 1 ps 1 lt 1 lw 2
set style line 11 lc rgb '#808080' lt 1
set style line 12 lc rgb '#808080' lt 0 lw 1
set border 3 back ls 11
set xrange [0:30]
set yrange [-1:1]
set sample 10000
set lmargin 50
g0(x)=((x>=0 && x<=5) ? 1 : 1/0)
g1(x)=((x>=5 && x<=10) ? 1 : 1/0)
g2(x)=((x>=10 && x<=15) ? 1 : 1/0)
g3(x)=((x>=15 && x<=20) ? 1 : 1/0)
g4(x)=((x>=20 && x<=25) ? 1 : 1/0)
g5(x)=((x>=25 && x<=30) ? 1 : 1/0)
 plot sin(0)*g0(x) with lines linestyle 1,sin(2*pi/9*x+0+(0-2*pi/9)*5)*g1(x) with lines linestyle 1,sin(2*pi/8*x+0+(0-2*pi/9)*5+(2*pi/9-2*pi/8)*10)*g2(x) with lines linestyle 1,sin(2*pi/7*x+0+(0-2*pi/9)*5+(2*pi/9-2*pi/8)*10+(2*pi/8-2*pi/7)*15)*g3(x) with lines linestyle 1,sin(2*pi/9*x+0+(0-2*pi/9)*5+(2*pi/9-2*pi/8)*10+(2*pi/8-2*pi/7)*15+(2*pi/7-2*pi/9)*20)*g4(x) with lines linestyle 1,sin(2*pi/9*25+0+(0-2*pi/9)*5+(2*pi/9-2*pi/8)*10+(2*pi/8-2*pi/7)*15+(2*pi/7-2*pi/9)*20)*g5(x) with lines linestyle 1
